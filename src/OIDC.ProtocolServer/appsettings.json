{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "ApplicationDbContext": "-TBD-"
  },

  "Authentication": {
    "OpenIdConnect": {
      "Authority": "-TBD-",
      "ClientId": "-TBD-",
      "ClientSecret": "-TBD-",
      "Resource": "",
      "Scopes": "openid",

      "SaveTokens": false,
      "GetClaimsFromUserInfoEndpoint": false
    },
    "OpenIdConnect-MFA": {
      "Authority": "-TBD-",
      "ClientId": "-TBD-",
      "ClientSecret": "-TBD-",
      "Resource": "",
      "Scopes": "openid",

      "SaveTokens": false,
      "GetClaimsFromUserInfoEndpoint": false
    }
  },

  "DistributedCache": {
    "ConnectionString": "-TBD-",
    "SchemaName": "dbo",
    "TableName": "DistritbutedCache"
  },

  "KeyStoreagePath": "-TBD-",
  "CertificatePassword": "-TBD-",

  "ProtocolServer": {
    "Issuer": "",

    "LdapServer": "-TBD-",
    "SubjectClaimType": "zdv_sub",

    "Properties": {
      "zdvStudentID": "string",

      "zdvCostCenter": "string",
      "zdvDateOfBirth": "string",
      "zdvIDCardImage": "byte[]",
      "zdvIDCardImageTimestamp": "DateTime",
      "zdvLibraryID": "string"
    },

    "ClaimSources": [
      {
        "ClaimType": "matriculation_number",
        "PropertyName": "zdvStudentId"
      },
      {
        "ClaimType": "role",
        "PropertyName": "msds-tokenGroupNamesGlobalAndUniversal",
        "ClaimValueDenyList": [ "^aobj_.+$", "^www-.+-m$" ]
      },
      {
        "ClaimType": "zdv_sid",
        "PropertyName": "objectSid",
        "OutputFormat": "sddl"
      },
      {
        "ClaimType": "zdv_sub",
        "PropertyName": "objectGuid",
        "OutputFormat": "guid"
      },
      {
        "ClaimType": "zdv_upn",
        "PropertyName": "userPrincipalName"
      },
      {
        "ClaimType": "zdv_uuid",
        "PropertyName": "serialNumber"
      }
    ],

    "_CSTemplate": {
      "ClaimType": "",
      "PropertyName": "",
      "ClaimValueDenyList": [ "", "" ]
    }
  }
}
