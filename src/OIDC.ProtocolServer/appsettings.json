{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "ApplicationDbContext": "-TBD-"
  },

  "Authentication": {
    "OpenIdConnect": {
      "Authority": "-TBD-",
      "ClientId": "-TBD-",
      "ClientSecret": "-TBD-",
      "Resource": "",
      "Scopes": "openid",

      "SaveTokens": false,
      "GetClaimsFromUserInfoEndpoint": false
    },
    "OpenIdConnect-MFA": {
      "Authority": "-TBD-",
      "ClientId": "-TBD-",
      "ClientSecret": "-TBD-",
      "Resource": "",
      "Scopes": "openid",

      "SaveTokens": false,
      "GetClaimsFromUserInfoEndpoint": false
    }
  },

  "DistributedCache": {
    "ConnectionString": "-TBD-",
    "SchemaName": "dbo",
    "TableName": "DistritbutedCache"
  },

  "KeyStoreagePath": "-TBD-",
  "CertificatePassword": "-TBD-",

  "ProtocolServer": {
    "Issuer": "",

    "SubjectClaimType": "zdv_sub",
    "PersonIdentifierClaimType": "zdv_uuid",

    "DefaultClaimTypes": [
      "amr",
      "mfa_auth_time",
      "mfa_available_methods"
    ],

    "PrincipalClaimProvider" : {
      "ClaimTypeMaps": [
        {
          "ClaimType": "amr"
        },
        {
          "ClaimType": "mfa_auth_time"
        },
        {
          "ClaimType": "mfa_available_methods"
        },
        {
          "ClaimType": "sub"
        },
        {
          "ClaimType": "sid",
          "AsClaimType": "zdv_sid"
        },
        {
          "ClaimType": "zdv_sub",
          "AsClaimType": "zdv_sub",
          "Transformation": "Base64DecodeGuid"
        }
      ]
    },

    "ActiveDirectory": {
      "LdapServer": "-TBD-",

      "Properties": {
        "zdvStudentID": "string",

        "zdvCostCenter": "string",
        "zdvDateOfBirth": "string",
        "zdvIDCardImage": "byte[]",
        "zdvIDCardImageTimestamp": "DateTime",
        "zdvLibraryID": "string"
      },

      "ClaimSources": [
        {
          "ClaimType": "sub",
          "PropertyName": "objectGuid",
          "OutputFormat": "guid"
        },
        {
          "ClaimType": "matriculation_number",
          "PropertyName": "zdvStudentId"
        },
        {
          "ClaimType": "role",
          "PropertyName": "msds-tokenGroupNamesGlobalAndUniversal",
          "OutputFormat": "cn",
          "ClaimValueDenyList": [ "^aobj_.+$", "^www-.+-m$" ],
          "Casing": "lower"
        },
        {
          "ClaimType": "zdv_accountName",
          "PropertyName": "sAMAccountName"
        },
        {
          "ClaimType": "zdv_sid",
          "PropertyName": "objectSid",
          "OutputFormat": "sddl"
        },
        {
          "ClaimType": "zdv_sub",
          "PropertyName": "objectGuid",
          "OutputFormat": "guid"
        },
        {
          "ClaimType": "zdv_upn",
          "PropertyName": "userPrincipalName"
        },
        {
          "ClaimType": "zdv_uuid",
          "PropertyName": "serialNumber"
        }
      ]
    },

    "JGUDirectory": {
      "DatabaseConnectionString": "-TBD-"
    },

    "_CSTemplate": {
      "ClaimType": "",
      "PropertyName": "",
      "ClaimValueDenyList": [ "", "" ]
    }
  }
}
